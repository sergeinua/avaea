          <button class="btn  btn-xs" type="button">$400-$600<span class="badge">27</span></button>
        </div>
      </div>

      <div class="mybucket" style="">
        <div class="col-xs-4" style="padding:0;">
          <a href="/result#dep_arr"  class="btn btn-default btn-block mycustomtab" role="button"><%= __('Departure')%></a>
          <button class="btn btn-xs" type="button">0-6am<span class="badge">49</span></button>
