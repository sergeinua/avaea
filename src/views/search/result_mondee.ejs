<% if (searchResult.length) { %>

<div class="container">
  <nav class="navbar navbar-default navbar-fixed-top"  style="margin-top:50px;z-index:10;">
    <div class="container-fluid">

        <div id="tiles" class="row">

              <div class="mybucket" id="price_tile">
                  <a id="mydefaultid" href="/result#price"  class="btn btn-default btn-block mycustomtab" role="button"><%= __('Price')%></a>
                  <ul class="list-group" style='padding:1px;'>
                    <li style='padding:1px;' class="list-group-item">$100-$200<span class="badge">49</span></li>
                    <li style='padding:1px;' class="list-group-item">$200-$300<span class="badge">39</span></li>
                    <li style='padding:1px;' class="list-group-item">$300-$400<span class="badge">19</span></li>
                    <li style='padding:1px;' class="list-group-item">$400-$600<span class="badge">27</span></li>
                  </ul>
              </div>

              <div class="mybucket"  id="departure_tile">
                  <a href="/result#dep_arr"  class="btn btn-default btn-block mycustomtab" role="button"><%= __('Departure')%></a>
                  <ul class="list-group" style='padding:1px;'>
                    <li style='padding:1px;' class="list-group-item">0-6am<span class="badge">49</span></li>
                    <li style='padding:1px;' class="list-group-item">6am-12pm<span class="badge">39</span></li>
                    <li style='padding:1px;' class="list-group-item">12pm-8pm<span class="badge">19</span></li>
                    <li style='padding:1px;' class="list-group-item">8pm-0<span class="badge">27</span></li>
                  </ul>
              </div>

              <div class="mybucket" id="arrival_tile">
                  <a href="/result#dep_arr"  class="btn btn-default btn-block mycustomtab" role="button"><%= __('Arrival')%></a>
                  <ul class="list-group" style='padding:1px;'>
                    <li style='padding:1px;' class="list-group-item">0-6am<span class="badge">49</span></li>
                    <li style='padding:1px;' class="list-group-item">6am-12pm<span class="badge">39</span></li>
                    <li style='padding:1px;' class="list-group-item">12pm-8pm<span class="badge">19</span></li>
                    <li style='padding:1px;' class="list-group-item">8pm-0<span class="badge">27</span></li>
                  </ul>
              </div>

              <div class="mybucket" id="airline_tile">
                  <a id="mydefaultid" href="/result#price"  class="btn btn-default btn-block mycustomtab" role="button"><%= __('Airline')%></a>
                  <ul class="list-group" style='padding:1px;'>
                    <li  style='padding:1px;' class="list-group-item">China Eastern<span class="badge">9</span></li>
                    <li  style='padding:1px;' class="list-group-item">Virgin America<span class="badge">3</span></li>
                    <li  style='padding:1px;' class="list-group-item">Tiara Air<span class="badge">19</span></li>
                    <li  style='padding:1px;' class="list-group-item">Fly BVI<span class="badge">27</span></li>
                  </ul>
              </div>

              <div class="mybucket" id="merchandising_tile">
                  <a href="/result#dep_arr"  class="btn btn-default btn-block mycustomtab" role="button"><%= __('Merchandising')%></a>
                  <ul class="list-group" style='padding:1px;'>
                    <li style='padding:1px;' class="list-group-item">Free WiFi <span class="badge">49</span></li>
                    <li style='padding:1px;' class="list-group-item">In seat video <span class="badge">1</span></li>
                    <li style='padding:1px;' class="list-group-item">In seat audio <span class="badge">7</span></li>
                    <li style='padding:1px;' class="list-group-item">10kg luggage <span class="badge">6</span></li>
                  </ul>
              </div>

              <div class="mybucket" id="duration_tile">
                  <a href="/result#duration" class="btn btn-default btn-block mycustomtab" role="button"><%= __('Duration')%></a>
                  <ul class="list-group" style='padding:1px;'>
                    <li style='padding:1px;' class="list-group-item">3h-6h<span class="badge">49</span></li>
                    <li style='padding:1px;' class="list-group-item">6h-10h<span class="badge">39</span></li>
                    <li style='padding:1px;' class="list-group-item">10h-12h<span class="badge">19</span></li>
                    <li style='padding:1px;' class="list-group-item">12h-14h<span class="badge">27</span></li>
                  </ul>
              </div>

          </div>

      <div class="selectedfilters row"></div>
    </div>
  </nav>
</div>

  <% iterator = 0; %>
  <% searchResult.forEach(function (sRes) { %>
<div class="row" style="padding-bottom:10px; border-top: 1px solid gray;background-color:#eee">
  <div class="col-xs-12">

    <div class="row">
      <div class="col-xs-12" style="padding: 2px;"><a href='/order' class="btn-success btn-sm">Buy
        <% if(sRes.currency == 'USD') { %>
        $<%= sRes.price %> <% } else { %><%= sRes.price %> <%= fare.currency %><% } %></a></div>
    </div>

    <% sRes.citypairs.forEach(function (pair, i) { %>
      <div class="row">
        <div class="col-sm-12"><strong><%= pair.from.code %>&rarr;<%= pair.to.code %></strong></div>
      </div>
      <% pair.flights.forEach(function (flight) { %>
      <div class="row">
        <div class="col-xs-2"><%= flight.number %></div>
        <div class="col-sm-1"><%= flight.from.code %>&rarr;<%= flight.to.code %></div>
        <div class="col-xs-6"><%= flight.airline %></div>
        <div class="col-sm-3"><span class="pull-right"><%= flight.from.time %> - <%= flight.to.time %></span></div>
      </div>
      <div class="row">
        <div class="col-sm-3"><span class="small">Booking Class: <%= flight.bookingClass %></span></div>
        <div class="col-sm-3"><span class="small">Cabin Class: <%= flight.cabinClass %></span></div>
        <div class="col-sm-6"><span class="small pull-right">
          Flight time: <%= flight.duration %>
          <% if (flight.noOfStops > 0) { %>
          <span class="text-danger">+<%= flight.noOfStops + __(' stops')%></span>
          <% } %></span>
        </div>
      </div>

    <% if (flight.merchandising) { %>
      <div class="row">
        <div class="col-xs-12">
          <button class="btn btn-xs btn-info btn-block mymorebutton" role="button" for="<%= iterator %>"><%= __('more')%></button>
        </div>
      </div>
      <div class="row hidden mymorecontent<%= iterator %>">
        <div class="panel panel-default">
          <div class="panel-heading">Merchandising Information</div>
          <div class="panel-body">
            <ul>
              <% fs.merchandising.forEach(function (item) { %>
              <li><%= item %></li>
              <% }) %>
            </ul>
          </div>
        </div>
      </div>
    <% } %>
      <% }); %>
    <% }); %>

  <% iterator++ %>
  </div>
</div>

  <% }) %>
<style>
    body {
      padding-bottom: 70px;
      padding-top: 200px;
    }
</style>

<div class="container">
    <nav class="navbar navbar-default navbar-fixed-bottom">
      <div class="container-fluid">
        <a href='/order' class="btn btn-lg btn-primary btn-block" >Buy</a>
      </div>
    </nav>
</div>
<% } else { %>
    <div class="alert alert-danger" role="alert"><%= __('Could not find anything') %></div>
    <a href="/search"  class="btn btn-lg btn-primary btn-block" role="button"><%= __('Back to search') %></a>
<% } %>
