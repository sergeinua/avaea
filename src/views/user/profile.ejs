<form action="user/update" method="post">
<h2 class="form-signin-heading">Update profile</h2>
    <input type="text" name="firstName" class="form-control" placeholder="First Name"/>
    <input type="text" name="middleName" class="form-control" placeholder="Middle Name" />
    <input type="text" name="lastName" class="form-control" placeholder="Last Name" />
    <input type="text" name="address" class="form-control" placeholder="Address" />

<!--In Case of Emergency Notify-->
    <input type="text" name="notifyContact.name" class="form-control" placeholder="In Case of Emergency Notify"/>
    <input type="text" name="notifyContact.phone" class="form-control" placeholder="Emergency Notify Phone"/>
<!--Name(s) of Family Members who might travel with you-->
    <div class="panel panel-default">
        <div class="panel-heading">Name(s) of Family Members who might travel with you</div>
        <div class="panel-body">
            <div id="travelWith">
            <hr class="hidden"/>
            <input type="text" name="travelWith.firstName[]" class="form-control" placeholder="First Name"/>
            <input type="text" name="travelWith.lastName[]" class="form-control" placeholder="Last Name"/>
            <label>Gender</label>
            <select name="travelWith.gender[]" class="form-control">
                <option value=''>please choose</option>
                <option value="Male">Male</option>
                <option value="Female">Female</option>
            </select>
            <label>Date of birth</label>
            <input type="date" name="travelWith.DateOfBirth[]" class="form-control"/>
        </div>
        <div class="panel-footer">
            <button class="btn btn-xs btn-info btn-block mymoreprofilebutton" role="button" for="travelWith"><%= __('One more')%></button>
        </div>
    </div>
<!--Airlines Frequent Flier Miles Programs-->
    <div class="panel panel-default">
        <div class="panel-heading">Airlines Frequent Flier Miles Programs</div>
        <div class="panel-body">
            <div id="milesPrograms">
            <hr class="hidden"/>
            <input type="text" name="milesPrograms.airlineName[]" class="form-control" placeholder="Airline Name"/>
            <input type="text" name="milesPrograms.accountNumber[]" class="form-control" placeholder="Account Number"/>
            </div>
        </div>
        <div class="panel-footer">
            <button class="btn btn-xs btn-info btn-block mymoreprofilebutton" role="button" for="milesPrograms"><%= __('One more')%></button>
        </div>
    </div>
<!--Airline Club Lounge Memberships-->
    <div class="panel panel-default">
        <div class="panel-heading">Airline Club Lounge Memberships</div>
        <div class="panel-body">
            <div id="loungeMembership">
            <hr class="hidden"/>
            <input type="text" name="loungeMembership.airlineName[]" class="form-control" placeholder="Airline Name"/>
            <input type="text" name="loungeMembership.membershipNumber[]" class="form-control" placeholder=" Club Membership Number"/>
            <label>Expiration Date</label>
            <input type="date" name="loungeMembership.expirationDate[]" class="form-control"/>
            </div>
        </div>
        <div class="panel-footer">
            <button class="btn btn-xs btn-info btn-block mymoreprofilebutton" role="button" for="loungeMembership"><%= __('One more')%></button>
        </div>
    </div>
<!--Employer-->
    <div class="panel panel-default">
        <div class="panel-heading">Employer</div>
        <div class="panel-body">
            <input type="text" name="employer.companyName[]" class="form-control" placeholder="Company Name"/>
            <input type="text" name="employer.address[]" class="form-control" placeholder="Address" />
            <input type="tel" name="employer.phone[]" class="form-control" placeholder="Phone number" />
            <input type="text" name="employer.position[]" class="form-control" placeholder="Position Held" />
            <label>Salary Range</label>
            <select name="employer.salary[]" class="form-control">
                <option value=''>please choose</option>
                <option value="$50K - $100K">$50K - $100K</option>
                <option value="$100K - $200K">$100K - $200K</option>
                <option value="$200K - $300K">$200K - $300K</option>
                <option value="Over $300K">Over $300K</option>
            </select>
            <label>Total Household Income</label>
            <select name="employer.salary[]" class="form-control">
                <option value=''>please choose</option>
                <option value="$50K - $100K">$50K - $100K</option>
                <option value="$100K - $200K">$100K - $200K</option>
                <option value="$200K - $300K">$200K - $300K</option>
                <option value="Over $300K">Over $300K</option>
            </select>
        </div>
    </div>
    <label>Ethnicity (Race)</label>
    <select name="ethnicity" class="form-control">
        <option value=''>please choose</option>
        <option value='European'>European</option>
        <option value='Mexican'>Mexican</option>
        <option value='Latin American'>Latin American</option>
        <option value='Eastern European'>Eastern European</option>
        <option value='South East Asian (India, Pakistan, Bangladesh)'>South East Asian (India, Pakistan, Bangladesh)</option>
        <option value='Chinese'>Chinese</option>
        <option value='Asian'>Asian</option>
        <option value='African'>African</option>
        <option value='South Pacific'>South Pacific</option>
    </select>
<!--Preferred Airlines-->
    <div class="panel panel-default">
        <div class="panel-heading">Preferred Airlines</div>
        <div class="panel-body">
            <div id="preferredAirlines">
            <hr class="hidden"/>
            <label>Preferred Airlines type</label>
            <select name="preferredAirlines.travelType[]" class="form-control">
                <option value=''>please choose</option>
                <option value="Domestic Short Haul Trips">Domestic Short Haul Trips</option>
                <option value="Domestic Long Haul Flights">Domestic Long Haul Flights</option>
                <option value="International Flights">International Flights</option>
            </select>
            <input type="text" name="preferredAirlines.airlineName[]" class="form-control" placeholder="Airline Name"/>
            </div>
        </div>
        <div class="panel-footer">
            <button class="btn btn-xs btn-info btn-block mymoreprofilebutton" role="button" for="preferredAirlines"><%= __('One more')%></button>
        </div>
    </div>
<!--Preferred Class of Service-->
    <div class="panel panel-default">
        <div class="panel-heading">Preferred Class of Service</div>
        <div class="panel-body">
            <label>Domestic for Personal or Leisure Travel</label>
            <select name="preferredClass.domesticPersonal[]" class="form-control">
                <option value=''>please choose</option>
                <option value="First">First</option>
                <option value="Business">Business</option>
                <option value="Premium Economy">Premium Economy</option>
                <option value="Economy">Economy</option>
            </select>
            <label>Domestic for Business Travel</label>
            <select name="preferredClass.domesticBusiness[]" class="form-control">
                <option value=''>please choose</option>
                <option value="First">First</option>
                <option value="Business">Business</option>
                <option value="Premium Economy">Premium Economy</option>
                <option value="Economy">Economy</option>
            </select>
            <label>International for Personal or Leisure Travel</label>
            <select name="preferredClass.internationalPersonal[]" class="form-control">
                <option value=''>please choose</option>
                <option value="First">First</option>
                <option value="Business">Business</option>
                <option value="Premium Economy">Premium Economy</option>
                <option value="Economy">Economy</option>
            </select>
            <label>International for Business Travel</label>
            <select name="preferredClass.internationalBusiness[]" class="form-control">
                <option value=''>please choose</option>
                <option value="First">First</option>
                <option value="Business">Business</option>
                <option value="Premium Economy">Premium Economy</option>
                <option value="Economy">Economy</option>
            </select>
        </div>
    </div>

    <label>Ethnicity (Race)</label>
    <select name="preferredSeat" class="form-control">
        <option value=''>please choose</option>
        <option value='Window'>Window</option>
        <option value='Aisle'>Aisle</option>
        <option value='Exit Row'>Exit Row</option>
    </select>

<button type="submit" class="btn btn-lg btn-primary btn-block" >Update</button>
</form>
