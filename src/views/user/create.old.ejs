<%
/* global profile_fields */
%>
<div id="onlynavbar" page="usercreate"></div>
<form action="/update" name="Profile" id="Profile" method="post" class="form profile">

  <div class="panel-group create" id="accordion" role="tablist" aria-multiselectable="true">

    <div id="UserProfile"></div>

    <div class="button-holder">
      <button type="submit" class="big-button" >Create</button>
    </div>

  </div>

</form>

<script type="text/javascript">
  UserProfileData = {};
  UserProfileStructure = {
    gender: <%- JSON.stringify(Profile.attr_gender) %>
  };
  UserProgramsStructure = {
    travel_type: <%- JSON.stringify(Profile.attr_travel_type) %>
  };
  var NavBarInit = {
    user: {email:"<%= user.email %>", id: <%= user.id %>}
  };
</script>
