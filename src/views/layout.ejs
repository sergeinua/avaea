          <% } %>
          }}();
        </script>
      </div>
    </div>
    
    <footer class="app-copy">
        <div class="top">
          <div class="wrapper">
            <div class="left">
              <nav>
                <ul class="menu nav">
                  <li class="active">
                    <a href="/home">Home</a>
                  </li>
                  <li class="">
                    <a href="/about">About</a>
                  </li>
                  <li class="">
                    <a href="/contact">Contact</a>
                  </li>
                </ul>
              </nav>
            </div>
            <div class="right">
              <a class="social fb" href="http://www.facebook.com/onvoya" target="_blank"></a>
              <a class="social twitter" href="http://www.twitter.com/onvoya" target="_blank"></a>
              <a class="social instagram" href="http://www.instagram.com/onvoya" target="_blank"></a>
            </div>
          </div>
        </div>
        <div class="bottom">
          <a href="/terms">Terms and Conditions</a>
          <a href="/privacy">Privacy</a>
        </div>
    </footer>

  </body>
</html>
