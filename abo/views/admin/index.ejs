<!DOCTYPE html>
<html lang="en">
  <head>
    <title>Demo</title>

    <!-- Viewport mobile tag for sensible mobile support -->
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">

    <!--STYLES-->

    <!--STYLES END-->
    <style  type="text/css">
      body { padding-top: 70px; }
    .twitter-typeahead {
        width: 99.9%;
    }
    .typeahead, .tt-query, .tt-hint {
    }
    .typeahead {
        background-color: #fff;
    }
    .typeahead:focus {
        border: 2px solid #0097cf;
    }
    .tt-query {
        box-shadow: 0 1px 1px rgba(0, 0, 0, 0.075) inset;
    }
    .tt-hint {
        color: #999;
    }
    .tt-menu {
        background-color: #fff;
        border: 1px solid rgba(0, 0, 0, 0.2);
        border-radius: 8px;
        box-shadow: 0 5px 10px rgba(0, 0, 0, 0.2);
        margin: 12px 0;
        padding: 8px 0;
    }
    .tt-suggestion, .empty-message {
        padding: 3px 10px;
    }
    .tt-suggestion:hover {
        background-color: #0097cf;
        color: #fff;
        cursor: pointer;
    }
    .tt-suggestion.tt-cursor {
        background-color: #0097cf;
        color: #fff;
    }
    .tt-suggestion p {
        margin: 0;
    }
    .input-group-btn {
      vertical-align: top;
    }

    .user-profile-button {
      text-align: center;
    }

    .company-icon {
      float: left;
      margin-top: 17px;
      margin-right: 5px;
    }
    </style>
  </head>

  <body>

<div class="container" style="padding:0">
  <nav class="navbar navbar-default navbar-fixed-top">
    <div class="container-fluid">
      <!-- Brand and toggle get grouped for better mobile display -->
      <div class="navbar-header">
        <button type="button" class="navbar-toggle collapsed pull-left" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
          <span class="sr-only">Toggle navigation</span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
        </button>

        <% if (typeof selectedAirline != 'undefined' && selectedAirline.length == 2) { %>
          <span class="company-icon">
            <img style="display: none;" src="/images/airlines/<%= selectedAirline %>.png" onload="this.style.display='block';"/>
          </span>
        <% } %>

        <a class="navbar-brand" href="/<%= selectedAirline %>">
          <%if (!title) { %><%= __('User info') %><% } else { %><%= title %><% } %>
        </a>
      </div>

      <!-- Collect the nav links, forms, and other content for toggling -->
      <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
        <ul class="nav navbar-nav">
          <li><a href="/<%= selectedAirline %>">User search</a></li>
          <% if (typeof user != 'undefined') { %>
          <li><a href="/logout">Log out <b><%= user.email %></b></a></li>
          <% } else { %>
          Powered by Demo Inc &copy; 2015
          <% } %>
        </ul>
      </div><!-- /.navbar-collapse -->
    </div><!-- /.container-fluid -->
  </nav>

</div>

<div class="main container clearfix">
    <!--<div id="statistic" class="hidden">-->
        <!--<div class="jumbotron">-->
          <!--<h1>Not implemented yet</h1>-->
        <!--</div>-->
    <!--</div>-->
    <div id="user_search" class="row">
        <form class="filter_user">
      <div class="input-group">
        <input id="user_search_form" type="text" name="user_id" class="form-control" placeholder="Search for user">
        <span class="input-group-btn">
          <button class="btn btn-default" type="submit" >Go!</button>
        </span>
        <script>
            var user_list = [
        <%
          users.forEach(function(user) {
        %>
                'ID#<%= user.id%> <%= user.email%>',
        <%
          });
        %>
        ''];
        </script>
      </div>
          </form>
      <input type="hidden" name="choosen_user" id="user_filter_value">


        <ul class="nav nav-pills">
            <li class="list-group-item-default" style="padding: 5px 5px 5px 5px">
              <label>
                Chart type
                <select id="chartSelection" for="on_chart_choice">
                  <option value="snowflake">Snowflake</option>
                  <option value="spiderchart">Spiderchart</option>
                </select>
              </label>
            </li>
            <li class=" list-group-item-info" style="padding: 5px 5px 5px 5px">
              <label>
                <input class="filters_checkbox" type="checkbox" for="on_tile_choice" checked> Tiles choice
              </label>
            </li>
            <li class=" list-group-item-success" style="padding: 5px 5px 5px 5px">
              <label>
                <input class="filters_checkbox" type="checkbox" for="order_tiles" checked> Order tiles
              </label>
            </li>
            <li class=" list-group-item-warning" style="padding: 5px 5px 5px 5px">
              <label>
                <input class="filters_checkbox" type="checkbox" for="order_itineraries" checked> Order itineraries
              </label>
            </li>
            <li class=" list-group-item-danger" style="padding: 5px 5px 5px 5px">
              <label>
                <input class="filters_checkbox" type="checkbox" for="on_itinerary_purchase" checked> Itinerary choice
              </label>
            </li>
            <li class=" list-group-item-default" style="padding: 5px 5px 5px 5px">
              <label>
                <input class="filters_checkbox" type="checkbox" for="prediction" checked> Prediction
              </label>
            </li>
          </ul>

      <div class="row">
          <div class="col-md-4">

              <canvas id="spider" height="350"></canvas>
            <!--<canvas id="snowflake" height="350"></canvas> old snowflake -->

            <h4>Slide show <button onclick="demo_auto()"> on/off</button></h4>

            <h4>Enter snowflake parameters or
              <button onclick="random_snowflake()">Generate a random snowflake</button>
            </h4>
            <form id="frm1">
              Num. sides <input type="number" name="nSides" value=10><br>
              Parameter A <input type="number" name="f1" value=1><br>
              Parameter B <input type="number" name="f2" value=2><br>
              Parameter C <input type="number" name="f3" value=6><br>
            </form>
            <button onclick="custom_snowflake()">Custom snowflake</button>
            <button onclick="animate_snowflake()">Animate the snowflake</button>
            <br>

            <canvas id="canvas1" width="500" height="500" style="border:1px solid teal;">
              Your browser does not support the HTML5 canvas tag.
            </canvas>

              <div id='chart_legend'></div>
              <div>
                <span class="user-profile-button input-group-btn" style="display: none;">
                  <button id="userProfileButton" class="btn btn-default" type="button" >User profile</button>
                </span>

              </div>
          </div>
          <div class="col-md-8" style="max-height: 500px; overflow: scroll;" id="log_table_div">
              <table class="table table-condensed" id="log_actions" style="width:100%;"></table>
          </div>
      </div>


    </div>
</div>

    <script src="/js/dependencies/jquery.js"></script>
    <script src="/js/dependencies/bootstrap.js"></script>
    <script src="/js/admin.js"></script>
    <script src="/js/dependencies/typeahead.bundle.min.js"></script>
    <script src="/js/dependencies/Chart.js"></script>
    <script src="/js/dependencies/snowflake.js"></script>

    <script type="text/javascript">
      <% if (selectedAirline) { %>
          var GlobalSelectedAirline = '<%=selectedAirline %>';
      <% } else { %>
        var GlobalSelectedAirline = null;
      <% } %>
      </script>

  </body>
</html>
